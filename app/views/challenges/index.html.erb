<h1>Listing challenges</h1>

<table>
  <thead>
    <tr>
      <th>Ticket</th>
      <th>Trailer</th>
      <th>Checkin</th>
      <th>Quiz</th>
      <th>Fblike</th>
      <th>Fbshare</th>
      <th>Tfollow</th>
      <th>Tweet</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @challenges.each do |challenge| %>
      <tr>
        <td><%= challenge.ticket %></td>
        <td><%= challenge.trailer %></td>
        <td><%= challenge.checkin %></td>
        <td><%= challenge.quiz %></td>
        <td><%= challenge.fblike %></td>
        <td><%= challenge.fbshare %></td>
        <td><%= challenge.tfollow %></td>
        <td><%= challenge.tweet %></td>
        <td><%= link_to 'Show', challenge %></td>
        <td><%= link_to 'Edit', edit_challenge_path(challenge) %></td>
        <td><%= link_to 'Destroy', challenge, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Challenge', new_challenge_path %>
